def is_isogram(s):
    a=set(s.lower())
    if len(s)==len(a):
        return True
    return False

##################USING FOR TWICE######################

def is_isogram(string):
    s=string.lower()
    for i in range(0,len(s)):
        for x in range(i+1,len(s)):
            if s[i]==s[x]:
                return False
    return True
